{
  "name": "@moukail/angular-zxing-scanner",
  "version": "9.0.1",
  "description": "High-performance Angular 9 barcode scanner component based on ZXing.",
  "private": false,
  "scripts": {
    "build": "ng-packagr -p ng-package.json",
    "lint": "tslint 'src/**/*.ts' --fix",
    "prebuild": "rimraf dist tmp",
    "reinstall": "rm -rf node_modules && npm install",
    "release": "npm run reinstall && npm run build && cd dist/angular-zxing-scanner && npm publish --access public && cd ../../"
  },
  "dependencies": {
    "@zxing/library": "^0.15.2"
  },
  "peerDependencies": {
    "@angular/core": "^9.0.0-rc.14",
    "@angular/common": "^9.0.0-rc.14",
    "@angular/forms": "^9.0.0-rc.14",
    "rxjs": "^6.5.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.900.0-rc.12",
    "@angular-devkit/build-ng-packagr": "^0.900.0-rc.12",
    "@angular/cli": "^9.0.0-rc.12",
    "@angular/compiler": "^9.0.0-rc.14",
    "@angular/compiler-cli": "^9.0.0-rc.14",
    "@angular/core": "^9.0.0-rc.14",
    "@angular/common": "^9.0.0-rc.14",
    "@angular/forms": "^9.0.0-rc.14",
    "@angular/language-service": "^9.0.0-rc.14",
    "@angular/platform-browser": "^9.0.0-rc.14",
    "@angular/platform-browser-dynamic": "^9.0.0-rc.14",
    "@types/node": "^13.7.0",
    "@types/qrcode": "^1.3.4",
    "codelyzer": "^5.2.1",
    "ng-packagr": "^5.7.1",
    "rimraf": "^3.0.1",
    "rxjs": "^6.5.4",
    "ts-node": "^8.6.2",
    "tslib": "^1.10.0",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5",
    "zone.js": "^0.10.2"
  }
}
